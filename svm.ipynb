    "from sklearn import svm\n",
    "X = [[0,0],[2,2]]\n",
    "y = [1,2]\n",
    "#初始化使用径向基核的分类器\n",
    "clf = svm.SVC(kernel=\"rbf\")\n",
    "clf.fit(X,y)\n",
    "t = [[2,1],[0,1]]\n",
    "print(clf.predict(t))\n",
    "#返回测试数据到超平面的距离\n",
    "print(clf.decision_function(t))\n"
   ]
  }
